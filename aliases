alias la='ls -al'
alias ls='ls -G'
alias ll='ls -lh'

alias pl='echo $LD_LIBRARY_PATH | tr ":" "\n" '
alias pp='echo $PATH | tr ":" "\n" '
alias v=mvim
alias vi=vim
alias sc='pushd .; cd ~; source .zshrc; popd'
alias xpp='xmlstarlet fo'
alias st='git sourcetree'
alias bx='bundle exec'
alias js='java org.mozilla.javascript.tools.shell.Main'


alias postgresstart='pg_ctl -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log start'
alias postgresstop='pg_ctl -m fast -D /usr/local/var/postgres -l /usr/local/var/postgres/server.log stop'
